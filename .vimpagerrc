syn on 
set nocompatible 
filetype off
set rtp+=~/.vim/bundle/vundle/\
call vundle#rc()
Bundle 'altercation/vim-colors-solarized'
let mapleader = ","
nmap <silent> ,/ :nohlsearch<CR>
set background=dark
colorscheme solarized
filetype on

let vimpager_disable_x11 = 1

set laststatus=0
set ruler
set foldmethod=manual
set foldlevel=99
set nolist
